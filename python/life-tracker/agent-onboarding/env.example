# Configurações da aplicação
APP_NAME=Life Tracker - Agente de Onboarding
APP_VERSION=2.0.0
DEBUG=false

# Configurações do servidor
HOST=0.0.0.0
PORT=8000

# Configurações de banco de dados
DATABASE_URL=postgresql://postgres:password@localhost:5432/life_tracker
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_NAME=life_tracker
DATABASE_USER=postgres
DATABASE_PASSWORD=password

# Configurações de Redis
REDIS_URL=redis://localhost:6379/0
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0

# Configurações de API externa
API_BASE_URL=http://localhost:3000
API_TIMEOUT=30

# Configurações de IA/ML
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Configurações do Agno Framework
AGNO_DEBUG_MODE=false
AGNO_MEMORY_ENABLED=true
AGNO_AGENTIC_MEMORY=true
AGNO_MODEL_ID=gpt-4
AGNO_MEMORY_TABLE_NAME=user_memories
AGNO_MEMORY_SCHEMA=agno_memory

# Configurações de ferramentas externas
TAVILY_API_KEY=your_tavily_api_key_here
TAVILY_SEARCH_DEPTH=basic
TAVILY_INCLUDE_DOMAIN=all

# Configurações de logging
LOG_LEVEL=INFO
LOG_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(message)s

# Configurações de templates
TEMPLATES_DIR=templates
DEFAULT_TEMPLATE=balanced_template

# Configurações de personalização
MAX_CUSTOMIZATION_SCORE=100.0
MIN_CONFIDENCE_THRESHOLD=60.0

# Configurações de cache
CACHE_TTL=3600
CACHE_MAX_SIZE=1000

# Configurações de rate limiting
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=3600

# Configurações de memória do Agno
AGNO_MEMORY_MAX_ENTRIES=10000
AGNO_MEMORY_CLEANUP_INTERVAL=86400
AGNO_MEMORY_EMBEDDING_DIMENSION=1536

# Configurações de performance
AGNO_MAX_TOOL_EXECUTION_TIME=30
AGNO_MAX_MEMORY_SEARCH_RESULTS=10
AGNO_MEMORY_SIMILARITY_THRESHOLD=0.8
